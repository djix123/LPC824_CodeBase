{
    "env": {
        "myDefaultIncludePath": [ "${workspaceFolder}/src", "${workspaceFolder}/cmsis" ]
    },
    "configurations": [
        {
            "includePath": [ "${myDefaultIncludePath}" ],
            "defines": [
                "__USE_CMSIS"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "intelliSenseMode": "macos-clang-x64",
            "compilerPath": "/opt/homebrew/bin/arm-none-eabi-gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14"
        }
    ],
    "version": 4
}
